{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "groupName": "AppFlowy",
      "groupSlug": "appflowy",
      "matchDatasources": ["docker"],
      "matchPackageNames": [
        "docker.io/appflowyinc/admin_frontend",
        "docker.io/appflowyinc/appflowy_ai",
        "docker.io/appflowyinc/appflowy_cloud",
        "docker.io/appflowyinc/appflowy_worker",
        "docker.io/appflowyinc/gotrue"
      ],
      "matchUpdateTypes": ["major","minor", "patch", "digest"],
      "automerge": false,
      "ignoreTests": true
    }
  ]
}